{
  "presets": [
    "react",
    ["env", {"modules": false}],
    "stage-2"
  ],
  "plugins": [
    "transform-async-to-generator",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "syntax-dynamic-import"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "transform-react-jsx-source"
      ]
    },
    "production": {}
  }
}